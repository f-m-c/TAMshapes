<Shape id="Dots" displayName="Dots" icon="icons/dots.png" system="true">
    <Properties>
        <PropertyGroup name="Dots properties">
            <Property name="box" type="Dimension">35,20</Property>
        </PropertyGroup>
    </Properties>
    <Behaviors>
        <For ref="dot1">
            <Attr>
                <Arg>"cx"</Arg>
                <Arg>0</Arg>
            </Attr>
            <Attr>
                <Arg>"cy"</Arg>
                <Arg>$box.h/2</Arg>
            </Attr>
        </For>
        <For ref="dot2">
            <Attr>
                <Arg>"cx"</Arg>
                <Arg>$box.w/2</Arg>
            </Attr>
            <Attr>
                <Arg>"cy"</Arg>
                <Arg>$box.h/2</Arg>
            </Attr>
        </For>
        <For ref="dot3">
            <Attr>
                <Arg>"cx"</Arg>
                <Arg>$box.w</Arg>
            </Attr>
            <Attr>
                <Arg>"cy"</Arg>
                <Arg>$box.h/2</Arg>
            </Attr>
        </For>
    </Behaviors>
    <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil"
               xmlns="http://www.w3.org/2000/svg">

        <circle
           id="dot1"
           style="color:#000000"
           r="3" />

        <circle
           id="dot2"
           style="color:#000000"
           r="3" />

        <circle
           id="dot3"
           style="color:#000000"
           r="3" />
    </p:Content>
</Shape>
